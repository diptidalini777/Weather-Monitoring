datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Weather {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  city          String
  temperature   Float
  feelsLike     Float
  mainCondition String
  createdAt     DateTime @default(now())
}
